<h1>Current users</h1>
<div id="notice" <% if flash[:warning] %>class="warning"<% end %>>
  <%= flash[:warning] || flash[:notice] %>
</div>
<table>
    <tr>
      <th> Name </th>
      <th> Email </th>
    </tr>
<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
  </tr>
<% end %>
</table>
</br>
</br>
</br>
<div class="info-message">
  <%= link_to "Add User", new_user_path %>
</div>
